    "import sys\n",
    "\n",
    "N = int(sys.stdin.readline())\n",
    "L = list(map(int, sys.stdin.readline().split()))\n",
    "dp = [1]*1001\n",
    "\n",
    "for i in range(1, N):\n",
    "    for j in range(i):\n",
    "        if L[i] > L[j]:\n",
    "            dp[i] = max(dp[j] + 1, dp[i])\n",
    "\n",
    "print(max(dp))"
   ]
  }
