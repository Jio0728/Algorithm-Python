      "source": [
        "import sys\n",
        "\n",
        "n = int(sys.stdin.readline())\n",
        "\n",
        "L = list(map(int, sys.stdin.readline().split()))\n",
        "dp = [0] * n\n",
        "dp[0] = L[0]\n",
        "for i in range(n):\n",
        "  dp[i] = max(dp[i-1] + L[i], L[i])\n",
        "\n",
        "print(max(dp))"
      ]
    }
  ]
